{
  "name": "capybara-router",
  "version": "2.0.3",
  "description": "Unfancy react router without flux and redux.",
  "main": "index.js",
  "scripts": {
    "start": "concurrently --kill-others \"node_modules/nodemon/bin/nodemon.js example/server.js --watch example/server.js\" \"node_modules/webpack-dev-server/bin/webpack-dev-server.js\"",
    "eslint": "./node_modules/eslint/bin/eslint.js ./ --ignore-path .gitignore --ignore-pattern 'example/web.js'",
    "build": "./node_modules/webpack/bin/webpack.js --mode=production",
    "changelog": "node_modules/conventional-changelog-cli/cli.js -p angular",
    "ncu": "./node_modules/npm-check-updates/bin/ncu",
    "test": "./node_modules/jest/bin/jest.js --coverage",
    "coveralls": "./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info"
  },
  "files": [
    "index.js",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kelp404/capybara-router.git"
  },
  "keywords": [
    "react",
    "router"
  ],
  "author": "Kelp <kelp@phate.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kelp404/capybara-router/issues"
  },
  "homepage": "https://github.com/kelp404/capybara-router#readme",
  "dependencies": {
    "history": "4.10.1",
    "prop-types": "15.7.2",
    "query-string": "6.12.1",
    "react": "16.13.1"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@babel/plugin-proposal-class-properties": "7.10.1",
    "@babel/polyfill": "7.8.7",
    "@babel/preset-env": "7.10.2",
    "@babel/preset-react": "7.9.4",
    "axios": "0.19.2",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-polyfill": "6.26.0",
    "classnames": "2.2.6",
    "concurrently": "5.1.0",
    "config": "3.3.1",
    "conventional-changelog-cli": "2.0.34",
    "coveralls": "3.1.0",
    "eslint": "6.8.0",
    "eslint-config-xo": "0.29.1",
    "eslint-config-xo-react": "0.23.0",
    "eslint-config-xo-space": "0.24.0",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "express": "4.17.1",
    "jest": "26.0.1",
    "nodemon": "2.0.4",
    "npm-check-updates": "6.0.1",
    "nprogress": "0.2.0",
    "react-dom": "16.13.1",
    "react-test-renderer": "16.13.1",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0"
  }
}
