    "react": "16.13.1"
  },
  "devDependencies": {
    "@babel/core": "7.14.2",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-react": "7.12.5",
    "axios": "0.21.1",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "babel-polyfill": "6.26.0",
    "classnames": "2.3.1",
    "concurrently": "5.3.0",
